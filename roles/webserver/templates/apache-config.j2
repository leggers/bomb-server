<VirtualHost *:80>
    ServerAlias {{group_names[0]}}{{item}}.bombsheller.com
    DocumentRoot /var/www/{{item}}/current/public/
    <Directory /var/www/{{item}}/current/public>
        AllowOverride all
        Options -MultiViews
        Order allow,deny
        Allow from all
    </Directory>
</VirtualHost>